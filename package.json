{
  "name": "@colbymchenry/react-auth-library",
  "version": "0.1.0",
  "description": "Firebase session-based authentication utilities for Next.js App Router projects",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    },
    "./next/app-router": {
      "types": "./dist/next/app-router/index.d.ts",
      "import": "./dist/next/app-router/index.js",
      "require": "./dist/next/app-router/index.cjs"
    },
    "./next/middleware": {
      "types": "./dist/next/middleware/index.d.ts",
      "import": "./dist/next/middleware/index.js",
      "require": "./dist/next/middleware/index.cjs",
      "edge-light": "./dist/next/middleware/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "prepare": "bun run build"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "firebase": ">=10.0.0",
    "firebase-admin": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "zustand": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "zustand": "^4.5.0"
  },
  "keywords": [
    "firebase",
    "authentication",
    "nextjs",
    "session",
    "auth",
    "middleware"
  ],
  "author": "Colby McHenry",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/colbymchenry/react-auth-library"
  }
}
